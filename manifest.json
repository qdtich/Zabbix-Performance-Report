{
    "manifest_version": 2.0,
    "id": "performance_report",
    "type": "module",
    "name": "Performance report",
    "version": "1.4",
    "namespace": "PerformanceReport",
    "author": "thinkc@outlook.com",
    "description": "Performance report for monitored hosts.",
    "actions": {
        "performance.report": {
            "class": "PerformanceReport",
            "view": "module.performance.report"
        },
        "performance.detail": {
            "class": "PerformanceDetail",
            "view": "layout.performance.detail"
        },
        "performance.export": {
            "class": "PerformanceExport",
            "view": "layout.performance.export"
        }
    }
}
